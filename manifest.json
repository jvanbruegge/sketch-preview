{
  "manifest_version": 2,
  "name": "sketch-preview",
  "version": "1.0",
  "description": "A browser extension to view sketch files in github",
  "background": {
    "scripts": ["build/background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "js": ["build/content.js"]
    }
  ],
  "permissions": ["*://*.github.com/"]
}
